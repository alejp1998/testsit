{
  "name": "quiz-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "heroku local:start",
    "super": "supervisor ./bin/www",
    "start_old": "node ./bin/www",
    "migrate_local": ": ${DATABASE_URL:=sqlite://$(pwd)/quiz.sqlite} ; npx sequelize-cli db:migrate --url $DATABASE_URL",
    "seed_local": ": ${DATABASE_URL:=sqlite://$(pwd)/quiz.sqlite} ; npx sequelize-cli db:seed:all --url $DATABASE_URL",
    "migrate_win": "sequelize db:migrate --url %DATABASE_URL%",
    "seed_win": "sequelize db:seed:all --url %DATABASE_URL%",
    "open_heroku": "heroku open",
    "migrate_heroku": "heroku run npx sequelize-cli db:migrate --url \\$DATABASE_URL",
    "seed_heroku": "heroku run npx sequelize-cli db:seed:all --url \\$DATABASE_URL",
    "reset_heroku": "heroku pg:reset DATABASE",
    "mon_heroku": "heroku maintenance:on",
    "moff_heroku": "heroku maintenance:off",
    "test": "NODE_ENV=testing mocha -R spec"
  },
  "engines": {
    "node": "10.19.0",
    "npm": "6.14.4"
  },
  "dependencies": {
    "bulma-start": "0.0.3",
    "chart.js": "^2.8.0",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-flash": "0.0.2",
    "express-partials": "^0.3.0",
    "express-session": "^1.16.1",
    "handlebars": "^4.7.6",
    "http-errors": "~1.6.2",
    "mathjax-node": "^2.1.1",
    "method-override": "^3.0.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.19.0",
    "pg": "^7.11.0",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "bulma": "^0.7.5",
    "node-sass": "^4.14.1",
    "sqlite3": "^4.2.0"
  }
}
